{
  "name": "astro-md-image-plugin",
  "packageManager": "pnpm@7.12.2",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://github.com/userquin/astro-md-image-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/userquin/astro-md-image-plugin.git"
  },
  "bugs": "https://github.com/userquin/astro-md-image-plugin/issues",
  "keywords": [
    "astro-integration",
    "astro",
    "markdown",
    "image"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./dist/*": "./dist/*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint-fix": "pnpm lint --fix",
    "xprepublishOnly": "npm run build",
    "xrelease": "npx bumpp --push --tag --commit && npm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@antfu/ni": "^0.18.0",
    "astro": "^1.5.0",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "eslint": "^8.23.0",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "vite": "^3.1.8"
  }
}
